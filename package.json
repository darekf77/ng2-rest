{
  "tnp": {
    "resources": [
      "README.md"
    ],
    "type": "isomorphic-lib",
    "overrided": {
      "dependencies": {
        "@types/diff": "^3.2.2",
        "axios": "^0.17.1",
        "diff": "^3.2.0"
      },
      "includeAsDev": [
        "morphi",
        "@angular.*",
        "webpack",
        "webpack-dev-server",
        "html-webpack-plugin",
        "ts-loader",
        "babel.*",
        "mocha",
        "@types/mocha",
        "chai",
        "ts-sinon"
      ],
      "includeOnly": [
        "@types/diff",
        "diff",
        "axios",
        "lodash",
        "@types/lodash",
        "ng2-logger",
        "json5",
        "lodash-walk-object",
        "typescript-class-helpers",
        "json10"
      ]
    }
  },
  "name": "ng2-rest",
  "version": "9.2.11",
  "scripts": {
    "build:watch": "npm-run watch \"morphi build\" src"
  },
  "bugs": {
    "url": "https://github.com/darekf77/ng2-rest/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-rest#readme",
  "keywords": [
    "isomorphic",
    "logger",
    "log",
    "typescript"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-rest.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "tnp deps:show:if:standalone"
    }
  }
}
